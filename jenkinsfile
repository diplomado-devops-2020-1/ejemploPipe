pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                bat './mvnw.cmd clean compile -e'
            }
        }

        stage('test') {
            steps {
                sh "/usr/local/opt/maven/bin/mvn compile"
            }
        }

        stage('test2') {
            steps {
                sh '''
                    /usr/local/opt/maven/bin/mvn test
                '''
            }
        }
    }
}
